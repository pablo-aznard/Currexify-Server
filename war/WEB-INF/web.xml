<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Currexify</servlet-name>
		<servlet-class>es.currexify.server.CurrexifyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Currexify</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>es.currexify.server.Profile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>es.currexify.server.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>Transaction</servlet-name>
	    <servlet-class>es.currexify.server.TransactionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Transaction</servlet-name>
	    <url-pattern>/transaction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Register</servlet-name>
		<servlet-class>es.currexify.server.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
  	<servlet>
 	    <servlet-name>Transaction</servlet-name>
 	    <servlet-class>es.currexify.server.TransactionServlet</servlet-class>
 	</servlet>
 	<servlet-mapping>
 	    <servlet-name>Transaction</servlet-name>
 	    <url-pattern>/transaction</url-pattern>
 	</servlet-mapping>
 	<servlet>
		<servlet-name>Contact</servlet-name>
		<servlet-class>es.currexify.server.ContactServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	    <welcome-file>index</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
